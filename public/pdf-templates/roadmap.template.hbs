      background: radial-gradient(ellipse at top, rgba(29, 209, 161, 0.15) 0%, transparent 50%),
                  radial-gradient(ellipse at bottom, rgba(185, 19, 114, 0.15) 0%, transparent 50%),
                  #000000;
      position: relative;
      overflow: hidden;
    }
    
    .cover-page::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: 
        radial-gradient(circle at 20% 80%, rgba(29, 209, 161, 0.1) 0%, transparent 50%),
        radial-gradient(circle at 80% 20%, rgba(185, 19, 114, 0.1) 0%, transparent 50%),
        radial-gradient(circle at 40% 40%, rgba(29, 209, 161, 0.05) 0%, transparent 50%);
      pointer-events: none;
    }
    
    .cover-header-enhanced {
      position: relative;
      z-index: 10;
      background: linear-gradient(135deg, rgba(29, 209, 161, 0.4) 0%, rgba(185, 19, 114, 0.4) 100%);
      border: 2px solid rgba(255, 255, 255, 0.3);
      border-radius: 30px;
      padding: 16px 32px;
      margin-bottom: 40px;
      box-shadow: 0 8px 32px rgba(29, 209, 161, 0.2);
      backdrop-filter: blur(10px);
    }
    
    .cover-title-enhanced {
      font-size: 32px;
      font-weight: 800;
      background: linear-gradient(135deg, #1DD1A1 0%, #B91372 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
      text-align: center;
      margin-bottom: 16px;
      text-shadow: 0 0 30px rgba(29, 209, 161, 0.3);
    }
    
    .cover-description-enhanced {
      font-size: 16px;
      color: #e5e7eb;
      line-height: 1.6;
      text-align: center;
      max-width: 500px;
      margin: 0 auto 40px auto;
      font-weight: 400;
    }
    
    .cover-summary-enhanced {
      background: linear-gradient(135deg, rgba(255, 255, 255, 0.1) 0%, rgba(255, 255, 255, 0.05) 100%);
      border: 1px solid rgba(29, 209, 161, 0.3);
      border-radius: 20px;
      padding: 32px;
      margin-bottom: 32px;
      backdrop-filter: blur(15px);
      box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3);
      position: relative;
    }
    
    .cover-summary-enhanced::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: linear-gradient(135deg, rgba(29, 209, 161, 0.05) 0%, rgba(185, 19, 114, 0.05) 100%);
      border-radius: 20px;
      pointer-events: none;
    }
    
    .cover-focus-enhanced {
      background: linear-gradient(135deg, rgba(29, 209, 161, 0.15) 0%, rgba(185, 19, 114, 0.15) 100%);
      border: 2px solid rgba(29, 209, 161, 0.4);
      border-radius: 20px;
      padding: 28px;
      backdrop-filter: blur(10px);
      box-shadow: 0 8px 32px rgba(29, 209, 161, 0.15);
      position: relative;
    }
    
    .cover-focus-enhanced::before {
      content: '';
      position: absolute;
      top: -2px;
      left: -2px;
      right: -2px;
      bottom: -2px;
      background: linear-gradient(135deg, #1DD1A1, #B91372);
      border-radius: 22px;
      z-index: -1;
      opacity: 0.3;
    }
    
    .cover-logo-enhanced {
      height: 40px;
      margin-bottom: 24px;
      filter: drop-shadow(0 4px 12px rgba(29, 209, 161, 0.3));
    }
    
    .archetype-badge-enhanced {
      display: inline-flex;
      align-items: center;
      gap: 8px;
      padding: 8px 16px;
      background: linear-gradient(135deg, rgba(29, 209, 161, 0.2) 0%, rgba(185, 19, 114, 0.2) 100%);
      border: 1px solid rgba(29, 209, 161, 0.4);
      border-radius: 25px;
      font-size: 14px;
      font-weight: 600;
      color: #1DD1A1;
      margin-bottom: 16px;
    }
    
    /* Footer */
    .footer {
      text-align: center;
      color: #9ca3af;
      font-size: 12px;
      margin-top: 40px;
      padding-bottom: 40px;
    }
    
    .footer p {
      margin-bottom: 4px;
    }
    
    @media print {
      html, body {
        background: #000000 !important;
        background-color: #000000 !important;
        -webkit-print-color-adjust: exact !important;
        print-color-adjust: exact !important;
        color-adjust: exact !important;
      }
      
      @page {
        background: #000000 !important;
        background-color: #000000 !important;
        margin: 0 !important;
      }
      
      * {
        -webkit-print-color-adjust: exact !important;
        print-color-adjust: exact !important;
        color-adjust: exact !important;
      }
    }
  </style>
</head>
<body style="background: #000000 !important; background-color: #000000 !important; margin: 0; padding: 0;">
  <!-- Page Footer - Fixed on all pages -->
  <div class="page-footer">
    <div class="page-footer-content">
      <img src="https://storage.googleapis.com/msgsndr/G9A67p2EOSXq4lasgzDq/media/68642fe27345d7e21658ea3b.png" alt="HOME" class="footer-logo">
      <div class="footer-text">homeformusic.app</div>
    </div>
  </div>

  <div class="page-container">
    <div class="container">
      
      <!-- PAGE 1: Enhanced Cover Page -->
      <div class="pdf-page cover-page">
        <div class="page-content" style="position: relative; z-index: 2;">
          <div style="display: flex; align-items: center; justify-content: center; gap: 16px; margin-bottom: 40px;">
            <img src="https://storage.googleapis.com/msgsndr/G9A67p2EOSXq4lasgzDq/media/68642fe27345d7e21658ea3b.png" alt="HOME" class="logo">
            <div class="bubble-header">
              <span class="sparkle">✨</span>
              <span>Your Music Creator Roadmap</span>
              <span class="sparkle-pink">✨</span>
            </div>
          </div>
          
          <div style="text-align: center; margin-bottom: 40px;">
            <h1 class="cover-title-enhanced">{{pathway.title}}</h1>
            <p class="cover-description-enhanced">{{pathway.description}}</p>
          </div>
          
          <!-- Enhanced Summary Section -->
          <div class="cover-summary-enhanced">
            <div style="position: relative; z-index: 2;">
              <h4 style="font-size: 20px; font-weight: 700; color: #ffffff; margin-bottom: 20px; text-align: center;">
                🧭 Your Creative Blueprint
              </h4>
              <div style="text-align: center;">
                <p style="color: #e5e7eb; font-size: 15px; line-height: 1.6; margin-bottom: 16px;">
                  <span style="color: #1DD1A1; font-weight: 600;">{{fuzzyScoresArray.[0].name}}</span> is your primary creative archetype at <span style="color: #B91372; font-weight: 600;">{{fuzzyScoresArray.[0].percentage}}%</span> alignment. 
                  {{#if (eq fuzzyScoresArray.[0].archetypeLevel 'Core')}}
                  Your {{fuzzyScoresArray.[0].name}} identity is crystal clear and drives your creative decisions.
                  {{else if (eq fuzzyScoresArray.[0].archetypeLevel 'Engine')}}
                  {{fuzzyScoresArray.[0].name}} powers your creativity, while you're still exploring other facets of your artistic identity.
                  {{else if (eq fuzzyScoresArray.[0].archetypeLevel 'Emerging')}}
                  You're growing into your {{fuzzyScoresArray.[0].name}} strengths, with room to develop this pathway further.
                  {{else}}
                  Your {{fuzzyScoresArray.[0].name}} traits represent untapped potential waiting to be unlocked.
                  {{/if}}
                </p>
                
                {{#if fuzzyScoresArray.[1]}}
                {{#if (gte fuzzyScoresArray.[1].percentage 55)}}
                <p style="color: #e5e7eb; font-size: 15px; line-height: 1.6; margin-bottom: 16px;">
                  Your secondary <span style="color: #1DD1A1; font-weight: 600;">{{fuzzyScoresArray.[1].name}}</span> traits (<span style="color: #B91372; font-weight: 600;">{{fuzzyScoresArray.[1].percentage}}%</span>) complement your primary path, creating unique creative opportunities.
                </p>
                {{/if}}
                {{/if}}
                
                <p style="color: #1DD1A1; font-size: 15px; font-weight: 600; margin-bottom: 16px;">
                  Strategy: 
                  {{#if pathwayBlend}}
                  {{#if (eq pathwayBlend.type 'hybrid')}}
                  Your versatility across multiple paths gives you unique opportunities that single-focused creators miss.
                  {{else if (eq pathwayBlend.type 'blend')}}
                  Focus 70% on your {{fuzzyScoresArray.[0].name}} strengths while developing your {{fuzzyScoresArray.[1].name}} skills as a strategic advantage.
                  {{else}}
                  Your clear {{fuzzyScoresArray.[0].name}} direction allows for deep specialization and expertise building.
                  {{/if}}
                  {{else}}
                  Your clear {{fuzzyScoresArray.[0].name}} direction allows for deep specialization and expertise building.
                  {{/if}}
                </p>
                
                <p style="color: #B91372; font-weight: 600; font-style: italic; text-align: center; margin-top: 20px; font-size: 16px;">
                  You're not trying to fit into systems — you're creating better ones.
                </p>
              </div>
            </div>
          </div>
          
          <!-- Enhanced Focus Section -->
          <div class="cover-focus-enhanced">
            <div style="position: relative; z-index: 2; text-align: center;">
              <h4 style="font-size: 20px; font-weight: 700; color: #ffffff; margin-bottom: 16px;">Focus Changes Everything</h4>
              <blockquote style="color: #1DD1A1; font-size: 18px; font-style: italic; margin: 0 0 16px 0; font-weight: 600;">
                "Be like a postage stamp - stick to one thing until you get there"
              </blockquote>
              <p style="color: #e5e7eb; font-size: 15px; line-height: 1.6; margin: 0;">
                While you may identify with multiple paths, <strong style="color: #1DD1A1;">focus creates breakthroughs</strong>. This roadmap reveals your highest-impact priority and the path that will <strong style="color: #ffffff;">accelerate your music career</strong>.
              </p>
            </div>
          </div>
        </div>
      </div>
      
      <!-- PAGE 2: Creative Profile -->
      <div class="pdf-page">
        <div class="page-content">
          <div style="text-align: center; margin-bottom: 32px;">
            <div class="bubble-header">
              <span class="sparkle">✨</span>
              <span>Your Creative Profile</span>
              <span class="sparkle-pink">✨</span>
            </div>
          </div>
          
          <h3 class="section-title" style="text-align: center; margin-bottom: 8px;">Your Creative Profile</h3>
          <p class="section-subtitle" style="text-align: center; margin-bottom: 32px;">Ranked by alignment with your answers</p>
          
          <div>
            {{#each fuzzyScoresArray}}
            <div class="archetype-card {{#if isFirst}}archetype-primary{{/if}} {{#if @index}}{{#if (eq @index 1)}}archetype-secondary{{/if}}{{/if}}">
              {{#if isFirst}}
              <div class="rank-indicator rank-primary">#1 PRIMARY</div>
              {{else if (eq @index 1)}}
              <div class="rank-indicator rank-secondary">#2 SECONDARY</div>
              {{/if}}
              
              <div class="archetype-header">
                <div class="archetype-left">
                  <div class="archetype-icon-container {{#if isFirst}}icon-primary{{/if}}">
                    <span class="archetype-icon">{{icon}}</span>
                  </div>
                  <div class="archetype-info">
                    <div class="archetype-name-row">
                      <span class="archetype-name {{#if isFirst}}name-primary{{/if}}">{{name}}</span>
                      <span class="archetype-level-icon">{{archetypeIcon}}</span>
                    </div>
                    <span class="archetype-level {{#if (eq archetypeLevel 'Core')}}level-core{{else if (eq archetypeLevel 'Engine')}}level-engine{{else if (eq archetypeLevel 'Emerging')}}level-emerging{{else}}level-hidden{{/if}}">
                      {{archetypeLevel}} • {{archetypeDescription}}
                    </span>
                  </div>
                </div>
                <div class="archetype-right">
                  <div class="percentage {{#if isFirst}}percentage-primary{{/if}}">{{percentage}}%</div>
                  <div class="alignment-text">alignment</div>
                </div>
              </div>
              
              <p class="archetype-description">{{description}}</p>
              
              {{#if isFirst}}
              <div class="archetype-details">
                <div class="detail-row">
                  <span class="detail-label">Key Traits:</span>
                  <span class="detail-text">{{traits}}</span>
                </div>
                <div class="detail-row">
                  <span class="detail-label">Shadow Side:</span>
                  <span class="detail-text">{{shadow}}</span>
                </div>
              </div>
              {{/if}}
            </div>
            {{/each}}
          </div>
        </div>
      </div>
      
      <!-- PAGE 3: Strategic Roadmap -->
      <div class="pdf-page">
        <div class="page-content">
          <div style="text-align: center; margin-bottom: 32px;">
            <div class="bubble-header">
              <span class="sparkle">✨</span>
              <span>Your Strategic Roadmap</span>
              <span class="sparkle-pink">✨</span>
            </div>
          </div>
          
          <h2 style="font-size: 24px; font-weight: 700; color: #ffffff; text-align: center; margin-bottom: 32px;">Your Blueprint for Breakthrough Success</h2>
          
          {{#each pathway.nextSteps}}
          {{#if (lt @index 4)}}
          <div class="roadmap-step">
            <div class="step-number">{{add @index 1}}</div>
            <div class="step-content">
              <h3 class="step-title">{{step}}</h3>
              <p class="step-detail">{{detail}}</p>
            </div>
          </div>
          {{/if}}
          {{/each}}
        </div>
      </div>
      
      <!-- PAGES 4-7: Detailed Action Steps (Do not change anything) -->
      {{#each pathway.nextSteps}}
      <div class="detail-section" style="padding: 60px 40px 80px 40px; page-break-before: always; margin: 0;">
        <!-- Page Header for each step -->
        <div style="text-align: center; margin-bottom: 24px;">
          <div class="page-header-badge">
            <span class="sparkle">✨</span>
            Step {{add @index 1}} Strategic Roadmap
            <span class="sparkle-pink">✨</span>
          </div>
        </div>
        <h2 class="detail-section-title">{{step}}</h2>
        
        <div class="detail-box">
          <h3 class="detail-box-title">
            <span class="why-icon">💡</span>
            Why This Matters
          </h3>
          <p style="color: #d1d5db; font-size: 14px; line-height: 1.5;">{{detail}}</p>
        </div>
        
        <div class="detail-box">
          <h3 class="detail-box-title">
            <span class="action-icon">📋</span>
            Your Action Items
          </h3>
          <div class="action-item">
            <span class="action-check">✅</span>
            <span class="action-text">{{step}}</span>
          </div>
          <div class="action-item">
            <span class="action-check">✅</span>
            <span class="action-text">Set specific, measurable goals for this step</span>
          </div>
          <div class="action-item">
            <span class="action-check">✅</span>
            <span class="action-text">Connect with others who have succeeded in this area</span>
          </div>
          <div class="action-item">
            <span class="action-check">✅</span>
            <span class="action-text">Document your progress and learnings</span>
          </div>
          <div class="action-item">
            <span class="action-check">✅</span>
            <span class="action-text">Track results and optimize your approach</span>
          </div>
        </div>
        
        <div class="detail-box">
          <h3 class="home-resources-title">
            <img src="https://storage.googleapis.com/msgsndr/G9A67p2EOSXq4lasgzDq/media/68642fe27345d7e21658ea3b.png" alt="HOME" class="home-logo">
            HOME Resources
          </h3>
          {{#each ../pathway.resources}}
          {{#if (lt @index 3)}}
          <div class="resource-item">{{this}}</div>
          {{/if}}
          {{/each}}
        </div>
      </div>
      {{/each}}
      
      <!-- LAST PAGE: HOME Connection -->
      {{#if pathway.homeConnection}}
      <div class="pdf-page">
        <div class="page-content">
          <div style="text-align: center; margin-bottom: 32px;">
            <div class="bubble-header">
              <span class="sparkle">✨</span>
              <span>Your HOME Connection</span>
              <span class="sparkle-pink">✨</span>
            </div>
          </div>
          
          <h2 class="home-connection-title" style="text-align: center; margin-bottom: 24px;">
            <img src="https://storage.googleapis.com/msgsndr/G9A67p2EOSXq4lasgzDq/media/68642fe27345d7e21658ea3b.png" alt="HOME" class="home-logo" style="height: 24px; margin-right: 12px;">
            Transform Your Plan Into Reality
          </h2>
          
          <!-- THE WHY -->
          <div style="text-align: center; margin-bottom: 24px; padding: 16px; background: rgba(255, 255, 255, 0.03); border: 1px solid rgba(255, 255, 255, 0.1); border-radius: 12px;">
            <p style="color: #d1d5db; font-size: 14px; line-height: 1.5; margin-bottom: 12px;">
              {{#if (eq fuzzyScoresArray.[0].key 'touring-performer')}}
              As a {{fuzzyScoresArray.[0].name}}, you need stages to practice, equipment to perfect your sound, and connections to book your next gig.
              {{else if (eq fuzzyScoresArray.[0].key 'creative-artist')}}
              As a {{fuzzyScoresArray.[0].name}}, you need creative spaces, content creation tools, and a community that understands your artistic vision.
              {{else if (eq fuzzyScoresArray.[0].key 'writer-producer')}}
              As a {{fuzzyScoresArray.[0].name}}, you need professional studios, industry connections, and collaborators who share your creative standards.
              {{else}}
              As a music creator, you need the right environment, tools, and community to turn your potential into success.
              {{/if}}
            </p>
            <p style="color: #ffffff; font-size: 14px; font-weight: 600;">Will you execute your plan alone, or accelerate your journey with the right support?</p>
          </div>
          
          <!-- THE WHAT - Main Products -->
          <div class="main-products">
            <h3 class="main-products-title" style="font-size: 18px; margin-bottom: 16px;">What We Offer</h3>
            <div class="products-grid">
              <div class="product-card">
                <span class="product-icon">🤝</span>
                <h4 class="product-title">Online Community</h4>
                <p class="product-description">1,000+ creators, weekly events, workshops, and mentorship opportunities</p>
              </div>
              
              <div class="product-card">
                <span class="product-icon">🎤</span>
                <h4 class="product-title">Studio/Rehearsal Space</h4>
                <p class="product-description">250-capacity venue, recording studios, and rehearsal spaces with 24/7 access</p>
              </div>
              
              <div class="product-card">
                <span class="product-icon">📈</span>
                <h4 class="product-title">Expert Guidance</h4>
                <p class="product-description">Industry mentorship, masterclasses, and personalized strategy sessions</p>
              </div>
            </div>
          </div>
          
          <!-- THE HOW - How to Connect -->
          <div class="connect-section">
            <h3 class="connect-title">Ready to Accelerate Your Journey?</h3>
            <p class="connect-description">
              Join HOME's community of ambitious music creators. Start with our free platform or book a strategy session.
            </p>
            <div class="connect-info">
              🌐 homeformusic.app • 📧 hello@homeformusic.app
            </div>
          </div>
          
          <!-- Disclaimer -->
          <div style="margin-top: 16px; padding: 12px; background: rgba(255, 255, 255, 0.02); border: 1px solid rgba(255, 255, 255, 0.1); border-radius: 8px;">
            <p style="color: #9ca3af; font-size: 11px; line-height: 1.4; text-align: center; margin: 0;">
              <strong style="color: #ffffff;">Important:</strong> This tool is developed by <span style="color: #1DD1A1;">homeformusic.app</span> as a creative guidance resource. No tool or person can promise or guarantee results in the music industry. Success depends on talent, dedication, market conditions, and many other factors beyond any assessment.
            </p>
          </div>
        </div>
      </div>
      {{/if}}
    </div>
  </div>
</body>
</html>